<h2>Celebrities!</h2>
{{#each celebrityArr}}

<h3>{{this.name}}</h3>
<a href="/celebrities/{{this._id}}">Learn more!</a>
<form action="/celebrities/{{this._id}}/delete" method="POST">
    <button>Delete</button>
</form>

<a href="/celebrities/{{this._id}}/edit"> EDIT </a>

{{/each}}
<a href="/celebrities/new"> Add your own celebs!</a>